{
  "compilerOptions": {
    "target": "ES6" ,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true ,
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true
  }
}
